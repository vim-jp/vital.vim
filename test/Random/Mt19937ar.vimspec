  Describe .srand()
    It sets the seed
      call M.srand(1)
      let g:a1 = M.rand()
      let g:a2 = M.rand()
      let g:a3 = M.rand()

      call M.srand(1)
      Assert Equals(g:a1, M.rand())
      Assert Equals(g:a2, M.rand())
      Assert Equals(g:a3, M.rand())

      call M.srand(2)
      Assert NotEquals(g:a1, M.rand())
